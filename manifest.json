{
  "name": "Se",
  "version": "0.1",
  "manifest_version": 2,
  "description": "links",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "browser_action": {
    "default_title": "Se",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}